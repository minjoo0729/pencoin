    <main>
        {{range .Blocks}}
        <section>
            <ul>
                <li>{{.Data}}</li>
                <li>{{.Hash}}</li>
                <li>{{.PrevHash}}</li>
            </ul>
        </section>
        {{end}}
    </main>
</body>
</html>